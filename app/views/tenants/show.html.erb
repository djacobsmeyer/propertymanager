</br>
<div class="row, container, col-sm-8">
  <div class="tabbable tabs-left">
   <ul class="nav nav-tabs">
     <li class="active"><a href="#leasedetails" data-toggle="tab">Lease Details</a></li>
     <li><a href="#payments" data-toggle="tab">Payments</a></li>
     <li><a href="#requests" data-toggle="tab">Requests</a></li>
   </ul>
   <div class="tab-content">
     <div class="tab-pane active" id="leasedetails">
       <ul class="list-group" style="list-style: none;">
         <li>
           <ul class="container, list-inline">
              <li>
                <h3><%= current_tenant.name %></h3>
              </li>
              <li>
                <%= link_to "Edit Tenant Details", edit_tenant_path(@tenant), class: 'btn btn-default' %>
              </li>
            </ul>
          </li>
         <li><%= current_tenant.email %> </li>
         <li>Lease Start Date: <%= current_tenant.leasestart %> Lease End Date: <%= current_tenant.leaseend %></li>
         <li>
           <address>
             <%= current_tenant.address %>
             <%= current_tenant.city %>, <%= current_tenant.state %>
             <%= current_tenant.zipcode %>
           </address>
         </li>
       </ul>
     </div>
     <div class="tab-pane" id="payments">
       <% @receivables.each do |rec| %>
          <div class="list-group">
            <h4 class="list-group-item-heading"><%= current_tenant.name %>, <%= rec.date %></h4>
            <p class="list-group-item-text">$<%= rec.amount %> Due on: <%= rec.duedate %></p>
          </div>
        <% end %>
     </div>
     <div class="tab-pane" id="requests">
       <%= render partial: 'requests/form' %>
       <%= render partial: 'requests/requests' %>
     </div>
   </div>
 </div>
</div>
